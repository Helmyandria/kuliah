   "source": [
    "def rpm(suhu):\n",
    "    if suhu < 25:\n",
    "        return 0\n",
    "    elif suhu > 30:\n",
    "        return 1500\n",
    "    else:\n",
    "        return (suhu-25)/5.0 * 1500\n",
    "\n",
    "suhu_list = (26, 25.75, 29, 28, 28.5, 31.75)\n",
    "for suhu in suhu_list:\n",
    "    print 'Jika suhu ', suhu, ' derajat, maka rpm = ', rpm(suhu)"
   ]
